{
  "name": "patrick-travel-mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "type-check": "tsc --noEmit",
    "build:dev:android": "eas build --profile development --platform android",
    "build:dev:ios": "eas build --profile development --platform ios",
    "build:preview:android": "eas build --profile preview --platform android",
    "build:preview:ios": "eas build --profile preview --platform ios",
    "build:prod:android": "eas build --profile production --platform android",
    "build:prod:ios": "eas build --profile production --platform ios",
    "submit:android": "eas submit --platform android --latest",
    "submit:ios": "eas submit --platform ios --latest"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@hookform/resolvers": "^3.9.1",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-google-signin/google-signin": "^13.0.0",
    "@react-navigation/native": "^7.1.18",
    "@tanstack/react-query": "^5.90.3",
    "axios": "^1.12.2",
    "date-fns": "^4.1.0",
    "expo": "~54.0.13",
    "expo-asset": "~12.0.9",
    "expo-auth-session": "~7.0.8",
    "expo-constants": "~18.0.9",
    "expo-crypto": "~15.0.7",
    "expo-dev-client": "~6.0.15",
    "expo-device": "^8.0.9",
    "expo-document-picker": "~14.0.7",
    "expo-file-system": "~19.0.17",
    "expo-font": "~14.0.9",
    "expo-image-picker": "~17.0.8",
    "expo-linking": "~8.0.8",
    "expo-local-authentication": "~17.0.7",
    "expo-localization": "^17.0.7",
    "expo-notifications": "~0.32.12",
    "expo-router": "~6.0.12",
    "expo-secure-store": "~15.0.7",
    "expo-sharing": "^14.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "expo-updates": "~29.0.12",
    "expo-web-browser": "~15.0.8",
    "firebase": "^12.4.0",
    "i18next": "^25.6.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.65.0",
    "react-i18next": "^16.0.1",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "~4.1.3",
    "react-native-safe-area-context": "5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-webview": "^13.15.0",
    "react-native-worklets": "0.5.1",
    "uploadthing": "^7.4.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@types/react": "~19.1.17",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "babel-preset-expo": "~12.0.8",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~10.0.0",
    "prettier": "^3.6.2",
    "typescript": "^5.3.3"
  },
  "expo": {
    "install": {
      "exclude": [
        "@react-navigation/native"
      ]
    }
  },
  "private": true
}
